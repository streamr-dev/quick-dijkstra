    "node-addon-api": "^1.0.0"
  },
  "devDependencies": {
    "jest": "^25.2.7",
    "js-graph-algorithms": "^1.0.18",
    "jsdoc-to-markdown": "^5.0.3",
    "typedoc": "^0.20.19",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  }
}
