{
  "name": "quick-dijkstra",
  "version": "1.0.0",
  "description": "Dijkstra Shortest Path Calculator implemented as a nodejs native module",
  "main": "quickdijkstra.js",
  "typings": "quickdijkstra.d.ts",
  "private": true,
  "gypfile": true,
  "scripts": {
    "test": "jest --runInBand",
    "speedtest": "node test/speedtest.js",
    "jsdoc": "jsdoc2md --template README.hbs --files ./quickdijkstra.js >README.md",
    "tsdoc": "npx typedoc --plugin typedoc-plugin-markdown",
